package bank.cash.counter;

public class Customer {

	double amount;
	
	public double getAmount() {
		return amount;
	}
	public void setAmount(double amount) {
		this.amount = amount;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Customer(String name, double amount) 
	{
		super();
		this.amount = amount;
		this.name = name;
	}
	String name; 
	
	
}
