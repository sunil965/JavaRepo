
public class InsertionSortInteger
{
	public static void main(String[] args) {
		int[] arr = {2,4,7,9,1,6,5};
		int n = arr.length;
		
		System.out.println("Array before  sorting...");
		for(int a:arr){
			System.out.print(a);
		}
		
		InsertionSort bs = new InsertionSort();
		bs.sort(arr, n);
		
		System.out.println("\n Array after sorting...");
		for(int a:arr){
			System.out.print(a+ " ");
		}
	}
	
	public static void sort(int a[] , int n){
		for(int i=1; i<n-1; i++){
			int data = a[i];
			int hole = i;
			while(hole>0 && a[hole-1]>data){
				a[hole] = a[hole-1];
				hole = hole-1;
			}
			a[hole] = data;
		}
	}

}
