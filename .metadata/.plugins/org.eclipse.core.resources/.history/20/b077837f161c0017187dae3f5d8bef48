package bank.cash.counter;

import java.util.ArrayList;
import java.util.Scanner;

public class Bank {

	double bankBalance = 50000;
	Scanner scan = new Scanner(System.in);
	
	static ArrayList list = new ArrayList();
	
	public static void main(String[] args)
	{
	 Customer customer1 = new Customer(5000);
	 Customer customer2 = new Customer(5000);
	 Customer customer3 = new Customer(5000);
	 Customer customer4 = new Customer(5000);
	 Customer customer5 = new Customer(5000);
	 list.add(customer1);
	 list.add(customer2);
	 list.add(customer3);
	 list.add(customer4);
	 list.add(customer5);
	}
	
	public void transaction(ArrayList list)
	{
	  for(int i=0;i<list.size();i++){
		  list.get(i);
	  }
	}
	
}
